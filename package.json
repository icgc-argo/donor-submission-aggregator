{
  "name": "donor-submission-aggregator",
  "version": "1.8.0",
  "description": "Donor aggregation service for the ICGC-ARGO Submission System",
  "main": "src",
  "scripts": {
    "build": "rm -rf dist && tsc && cp -r src/assets dist",
    "start::prod": "cd dist && NODE_PATH=. node index.js",
    "start::dev": "ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "test": "npm run build && NODE_PATH=./dist nyc -- mocha --timeout 240000 dist/**/*.test.js && npm run cover",
    "createMongoDonors": "ts-node ./scripts/createMongoDonors.ts",
    "cover": "nyc report  --reporter=lcov --reporter=text",
    "createIntegrationTestMongoDonors": "ts-node ./scripts/createTestDonors.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "**/*.test.ts",
      "scripts/**"
    ],
    "reporter": [
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icgc-argo/donor-submission-aggregator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/icgc-argo/donor-submission-aggregator/issues"
  },
  "homepage": "https://github.com/icgc-argo/donor-submission-aggregator#readme",
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/dockerode": "^2.5.22",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.1",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.7.0",
    "@types/node-fetch": "^2.5.5",
    "@types/promise-retry": "^1.1.3",
    "@types/request": "^2.48.4",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^3.4.7",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.2.0",
    "cliui": "^7.0.1",
    "coveralls": "^3.0.9",
    "mocha": "^7.2.0",
    "node-duration": "^1.0.4",
    "node-fetch": "^2.6.1",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "testcontainers": "^2.20.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "uuid": "^3.4.0",
    "yargs": "^16.0.3"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "dotenv": "^8.2.0",
    "elastic-builder": "^2.7.1",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "kafkajs": "^1.13.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.8.11",
    "node-vault": "^0.9.13",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "promise-retry": "^1.1.1",
    "swagger-ui-express": "^4.1.3",
    "ts-hashcode": "^1.0.2",
    "url-join": "^4.0.1",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  }
}
